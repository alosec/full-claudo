{
  "name": "full-claudo",
  "version": "1.0.0",
  "description": "Simple multi-agent Claude system",
  "main": "dist/src/index.js",
  "bin": {
    "claudo": "./dist/src/cli.js"
  },
  "scripts": {
    "build": "tsc",
    "start": "npm run build && claudo up",
    "stop": "claudo down",
    "logs": "claudo logs",
    "postinstall": "npm run build",
    "test:parser": "npm run build && node dist/test/parser.test.js",
    "test:manager": "npm run build && node dist/src/test-manager.js",
    "test:complex": "npm run build && node dist/src/test-manager-complex.js",
    "test": "npm run test:parser"
  },
  "keywords": ["claude", "multi-agent", "automation"],
  "author": "Alex",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "typescript": "^5.0.0"
  }
}